{
  "version": "2.0",
  "extensions": {
    "blobs": {
      "maxDegreeOfParallelism": 1
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Error",
      "Host.Results": "Error",
      "Function": "Information",
      "Microsoft": "Error"
    }
  },
  "concurrency": {
    "dynamicConcurrencyEnabled": true,
    "snapshotPersistenceEnabled": true
  }
}